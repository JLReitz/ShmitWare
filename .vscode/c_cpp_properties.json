{
    "env": {
        "Development_Includes": "${workspaceFolder}/Development/Include",
        "ShmitPort_Root": "${workspaceFolder}/Development/Port",
        "Native_Defines": [
            "NATIVE_SHMIT"
        ]
    },
    "configurations": [
        {
            "name": "native-c++17",
            "includePath": [
                "${Development_Includes}",
                "${ShmitPort_Root}/Native"
            ],
            "defines": [
                "${Native_Defines}"
            ],
            "compilerPath": "/usr/bin/g++",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "linux-gcc-x64",
            "configurationProvider": "ms-vscode.cmake-tools"
        },
        {
            "name": "native-c++14",
            "includePath": [
                "${Development_Includes}",
                "${ShmitPort_Root}/Native"
            ],
            "defines": [
                "${Native_Defines}"
            ],
            "compilerPath": "/usr/bin/g++",
            "cStandard": "c11",
            "cppStandard": "c++14",
            "intelliSenseMode": "linux-gcc-x64",
            "configurationProvider": "ms-vscode.cmake-tools"
        },
        {
            "name": "zephyr-c++17",
            "includePath": [
                "${Development_Includes}",
                "${ShmitPort_Root}/Zephyr"
            ],
            "defines": [],
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "linux-gcc-x64",
        }
    ],
    "version": 4
}