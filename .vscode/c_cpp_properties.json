{
    "env": {
        "Include_Shmitcore": "${workspaceFolder}/Development/ShmitCore/Include",
        "Include_Shmitport": "${workspaceFolder}/Development/ShmitPort/Include",
        "Native_Defines": [
            "NATIVE_SHMIT"
        ]
    },
    "configurations": [
        {
            "name": "native-c++17",
            "includePath": [
                "${Include_Shmitcore}"
            ],
            "defines": [
                "${Native_Defines}"
            ],
            "compilerPath": "/usr/bin/g++",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "linux-gcc-x64",
            "configurationProvider": "ms-vscode.cmake-tools"
        },
        {
            "name": "native-c++14",
            "includePath": [
                "${Include_Shmitcore}"
            ],
            "defines": [
                "${Native_Defines}"
            ],
            "compilerPath": "/usr/bin/g++",
            "cStandard": "c11",
            "cppStandard": "c++14",
            "intelliSenseMode": "linux-gcc-x64",
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}